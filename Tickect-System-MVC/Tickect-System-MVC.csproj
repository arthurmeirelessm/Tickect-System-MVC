    <RootNamespace>Tickect_System_MVC</RootNamespace>
    <CopyRefAssembliesToPublishDirectory>false</CopyRefAssembliesToPublishDirectory>
  </PropertyGroup>

  <ItemGroup>
    <Content Remove="wwwroot\stylesheet\StyleSheetHome.css" />
    <Content Remove="wwwroot\stylesheet\StyleSheetWithoutUserLoggedRegisterUser.css" />
  </ItemGroup>

  <ItemGroup>
    <None Include="wwwroot\stylesheet\StyleSheetHome.css" />
    <None Include="wwwroot\stylesheet\StyleSheetWithoutUserLoggedRegisterUser.css" />
